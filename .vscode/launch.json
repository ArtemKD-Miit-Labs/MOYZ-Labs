{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "KP debug NASM",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/KP/Build/main-gdb",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-pronting",
                    "text": "-enable pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}